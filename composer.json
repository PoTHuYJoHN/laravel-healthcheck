{
    "name": "webkid/laraveldiagnostic",
    "description": "Diagnostic of main components: DB, Queue etc",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Kolodii",
            "email": "kolodiy.ivan.i@gmail.com",
            "homepage": "https://ikolodiy.com"
        }
    ],
    "homepage": "https://github.com/webkid/laraveldiagnostic",
    "keywords": ["Laravel", "LaravelDiagnostic"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webkid\\LaravelDiagnostic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webkid\\LaravelDiagnostic\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkid\\LaravelDiagnostic\\LaravelDiagnosticServiceProvider"
            ],
            "aliases": {
                "LaravelDiagnostic": "Webkid\\LaravelDiagnostic\\Facades\\LaravelDiagnostic"
            }
        }
    }
}
